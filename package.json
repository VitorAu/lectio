{
  "name": "lectio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --watch src/app.ts",
    "build": "node esbuild.config.ts",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose stop",
    "knex:latest": "node -r dotenv/config ./node_modules/.bin/knex migrate:latest --knexfile src/database/config/knexfile.ts",
    "knex:up": "node -r dotenv/config ./node_modules/.bin/knex migrate:up --knexfile src/database/config/knexfile.ts",
    "knex:down": "node -r dotenv/config ./node_modules/.bin/knex migrate:down --knexfile src/database/config/knexfile.ts",
    "test": "npx tap --disable-coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@fastify/jwt": "^10.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.5.2",
    "esbuild": "0.25.10",
    "fastify-pino-pretty": "^5.2.0",
    "tap": "^21.1.0",
    "ts-mock-imports": "^1.3.17",
    "typescript": "^5.9.2"
  }
}
